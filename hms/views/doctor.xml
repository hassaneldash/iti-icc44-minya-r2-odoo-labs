<odoo>
    <data>
        <record id="doctor_form_view" model="ir.ui.view">
            <field name="name">hms.doctor.form.view</field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="first_name"/>
                            <field name="last_name"/>
                            <field name="image"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="doctor_tree_view" model="ir.ui.view">
            <field name="name">hms.doctor.tree.view</field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="first_name"/>
                    <field name="last_name"/>
                    <field name="image"/>
                </tree>
            </field>
        </record>


        <record id="action_hms_doctor_all" model="ir.actions.act_window">
            <field name="name">All Doctors</field>
            <field name="res_model">hms.doctor</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
        </record>

        <menuitem id="menu_hms_doctor_all" name="All Doctors" parent="menu_root_doctor" action="action_hms_doctor_all"/>
    </data>
</odoo>